{
	"printWidth": 160,
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "ignore",
	"jsxBracketSameLine": true,
	"bracketSameLine": false,
	"endOfLine": "auto"
}
